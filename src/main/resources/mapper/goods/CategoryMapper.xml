<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.beauty4u.backend.goods.query.mapper.CategoryQueryMapper">

    <select id="findAllTopCategoryList" resultType="TopCategoryResDTO">
        SELECT
            top_category_code,
            top_category_name
        FROM top_category
        ORDER BY top_category_name
    </select>

</mapper>